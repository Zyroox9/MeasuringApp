<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.distancemeasurment">

    <uses-permission android:name="android.permission.CAMERA"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_measuring_tape"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_measuring_tape_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DistanceMeasurment">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".PopHelp"
            android:theme="@style/Theme.DistanceMeasurment.CustomTheme"
            ></activity>
    </application>

</manifest>